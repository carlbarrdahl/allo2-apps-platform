    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.12",
    "@types/node": "^20.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint-config-custom": "workspace:*",
    "react": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.33.0",
    "allo-v2-sdk": "workspace:*",
    "viem": "^1.9.5"
  }
}
