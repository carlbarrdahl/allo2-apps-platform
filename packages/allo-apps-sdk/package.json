{
  "name": "allo-apps-sdk",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup index.tsx --format esm,cjs --dts --external react",
